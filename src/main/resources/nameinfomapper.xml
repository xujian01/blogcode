<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="nameinfo">
    <select id="findByName" resultType="NameInfo">
        select id,name,create_time as createTime from name_info where name = #{name}
    </select>
</mapper>